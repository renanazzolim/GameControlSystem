<Window x:Class="View.Telas.Cadastrar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:View.Telas"
        mc:Ignorable="d"
        Title="Cadastrar" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="edtLogradouro" HorizontalAlignment="Left" Height="23" Margin="194,75,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="edtNumero" HorizontalAlignment="Left" Height="24" Margin="194,116,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="edtComplemento" HorizontalAlignment="Left" Height="24" Margin="194,160,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="edtBairro" HorizontalAlignment="Left" Height="24" Margin="194,202,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="Confirma_Btn" Content="Confirmar" HorizontalAlignment="Left" Margin="222,245,0,0" VerticalAlignment="Top" Width="74" Click="Confirma_Btn_Click"/>
    </Grid>
</Window>
